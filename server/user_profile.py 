import setup
from setup import Error

token = setup.generateToken('hayden@gmail.com', '123456', 'Hayden', 'Smith')
channel_id = setup.generateChannel(token, "Channel 1", True)
message_id = setup.generateMessage(token, channel_id, message)

def user_profile(token, u_id):
    u_id_dic = {'123': '123', 456': '456', '789': '789', '159': '159', 'AD': 'AD'}
    
    if u_id_dic[u_id] == token:
        if u_id in u_id_dic: 
            # hardcoded it to test
            if u_id == '123':
                return {'hayden@gmail.com', 'Hayden', 'Smith', 'handle'}
            elif u_id == '456':
                return {'smith@gmail.com', 'Smith', 'Hayden', 'handler'}
        else:
            raise ValueError("This username is not existed. Please try again")
        
    else:
        # u_id's token doesn't match the given token
        raise Error("Invalid token or u_id")

    # This is psedo-code
    return {email, name_first, name_last, handle_str}
